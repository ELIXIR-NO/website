    "@astrojs/tailwind": "^5.1.0",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.3",
    "@tailwindcss/forms": "^0.5.7",
    "animate.css": "^4.1.1",
    "astro": "^4.7.0",
    "dayjs": "^1.11.12",
    "dompurify": "^3.1.7",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
